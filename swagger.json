            "description": "device id kendaraan",
            "required": true,
            "type": "integer",
            "format": "int64"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/VehicleObject"
            }
          },
          "404": {
            "description": "No Vehicle Data"
          }
        }
      },
      "delete": {
        "summary": "menghapus data kendaraan",
