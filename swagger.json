  "paths": {
    "/vehicle": {
      "get": {
        "summary": "menampilkan semua kendaraan",
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/VehicleObject"
            }
          },
          "404": {
            "description": "No Vehicle Data"
          }
        }
      },
      "post": {
        "summary": "menambah kendaraan baru",
        "parameters": [
          {
            "$ref": "#/parameters/VehicleAddParam"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "properties": {
                "deviceId": {
                  "type": "integer"
                },
                "name": {
                  "type": "string"
                }
              }
            }
          },
          "404": {
            "description": "Imei ini sudah terdaftar !"
          }
        }
      },
      "put": {
        "summary": "mengedit data kendaraan",
        "parameters": [
          {
            "$ref": "#/parameters/VehicleEditParam"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "properties": {
                "deviceId": {
                  "type": "integer"
                },
                "name": {
                  "type": "string"
                }
              }
            }
          },
          "404": {
            "description": "Failed"
          }
        }
      }
    },
